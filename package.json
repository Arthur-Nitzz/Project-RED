{
  "name": "kaisermann.me",
  "version": "2.0.0",
  "author": "Christian Kaisermann",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=production eleventy",
    "build:dev": "eleventy",
    "build:prod": "run-s build purge-css critical-css",
    "lint": "eslint --ignore-pattern=\"node_modules\" .",
    "format": "prettier --write \"**/*.{js,json}\"",
    "critical-css": "./scripts/critical-css.sh",
    "purge-css": "purgecss -c purgecss.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaisermann/kaisermann.me"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.2.0",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@kiwi/eslint-config": "^1.11.3",
    "@kiwi/prettier-config": "^1.11.3",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "axios": "^0.21.1",
    "critical": "^3.0.1",
    "cross-env": "7.0.3",
    "cssnano": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-kiwi": "^1.3.4",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-extend-rule": "^3.0.0",
    "postcss-import": "^14.0.1",
    "postcss-load-config": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "purgecss": "^4.0.3",
    "rollup": "^2.45.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.37.0",
    "svelte-preprocess": "^4.7.2",
    "terser": "^5.6.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "js-levenshtein": "^1.1.6"
  }
}
